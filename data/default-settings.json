{
    "general": {
        "notify-srs": true,
        "notify-srs-interval": 900,
        "do-regular-backup": false,
        "regular-backup-interval": "1 week"
    },
    "languages": {
        "default": ""
    },
    "test": {
        "useFlashcardMode": false,
        "makeContinuous": false,
        "skipEvaluationOnCorrect": false,
        "enableIgnoreShortcut": false,
        "showProgress": false,
        "showScore": false,
        "animate": true,
        "useSerifFont": false,
        "useBackgroundColors": false,
        "fontSize": "normal"
    },
    "design": {
        "animateSlidingPanels": true,
        "animatePopupStacks": true,
        "fadeSectionSwitching": true,
        "cursor": "default",
        "colorScheme": "default"
    },
    "srs": {
        "intervalModifier": "4 hours"
    },
    "add": {
        "separator": ","
    },
    "shortcuts": {
        "force-quit": "CmdOrCtrl+Esc",
        "quit": "CmdOrCtrl+Q",
        "add-vocab": "CmdOrCtrl+A",
        "add-kanji": "CmdOrCtrl+K",
        "dictionary": "CmdOrCtrl+F",
        "test": "CmdOrCtrl+T",
        "help": "F1",
        "ignore-answer": "CmdOrCtrl+I",
        "fullscreen": "F11"
    }
}
