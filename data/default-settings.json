{
    "general": {
        "autoLaunchOnStartup": false,
        "autoLoadLanguageContent": true,
        "showSrsNotifications": false,
        "srsNotificationsInterval": 900,
        "doRegularBackup": true,
        "regularBackupInterval": "604800"
    },
    "test": {
        "useFlashcardMode": false,
        "makeContinuous": true,
        "skipEvaluationOnCorrect": false,
        "enableIgnoreShortcut": true,
        "showProgress": true,
        "showScore": false,
        "animate": true,
        "useSerifFont": false,
        "fontSize": "normal",
        "useBackgroundColors": false,
        "sortByLevel": false
    },
    "design": {
        "animateSlidingPanels": true,
        "animatePopupStacks": true,
        "fadeSectionSwitching": true,
        "enableAnimations": true,
        "cursor": "default",
        "colorScheme": "default",
        "testColorScheme": "default",
        "customTestUnicolorScheme": {
            "color": "f5f5f5",
            "background-color": "a93434"
        },
        "customTestMulticolorScheme": {
            "colors": {
                "word-meaning": "f5f5f5",
                "word-reading": "f5f5f5",
                "kanji-meaning": "f5f5f5",
                "kanji-on-yomi": "f5f5f5",
                "kanji-kun-yomi": "f5f5f5",
                "hanzi-meaning": "f5f5f5",
                "hanzi-reading": "f5f5f5"
            },
            "background-colors": {
                "word-meaning": "a93434",
                "word-reading": "356288",
                "kanji-meaning": "7b0853",
                "kanji-on-yomi": "483d8b",
                "kanji-kun-yomi": "556b2f",
                "hanzi-meaning": "7b0853",
                "hanzi-reading": "483d8b"
            }
        }
    },
    "dictionary": {
        "partOfSpeechInJapanese": false,
        "showFrequencyValues": false,
        "includeProperNameSearch": false,
        "historySizeLimit": 200,
        "frequencyWeights": {
            "news": 1,
            "book": 0,
            "net": 0
        }
    },
    "kanjiOverview": {
        "splittingCriterion": "grade",
        "sortingCriterion": "alphabetical",
        "onlyMissing": false,
        "showJouyou": true,
        "showJinmeiyou": false,
        "showHyougai": false
    },
    "kanjiSearch": {
        "searchCriterion": "yomi",
        "historySizeLimit": 100
    },
    "kanjiInfo": {
        "maximized": false,
        "historySizeLimit": 50
    },
    "srs": {
        "intervalModifier": "0 hours"
    },
    "pinwall": [
        { "type": "srs-status-overview" },
        { "type": "srs-review-schedule" }
    ],
    "shortcuts": {
        "quit": "CmdOrCtrl+Q",
        "close-topmost": "Escape",
        "toggle-bars-visibility": "F11",
        "refresh": "CmdOrCtrl+R",
        "save-data": "CmdOrCtrl+S",
        "open-dev-tools": "F12",

        "add-word": "CmdOrCtrl+A",
        "add-kanji": "CmdOrCtrl+Shift+A",
        "open-test-section": "CmdOrCtrl+T",
        "open-dictionary": "CmdOrCtrl+F",
        "open-kanji-search": "CmdOrCtrl+Shift+F",
        "open-kanji-overview": "CmdOrCtrl+K",
        "open-vocab-section": "CmdOrCtrl+W",
        "open-notes-section": "CmdOrCtrl+N",

        "open-home-section": "CmdOrCtrl+H",
        "open-stats-section": "CmdOrCtrl+P",
        "open-settings": "CmdOrCtrl+E",
        "open-help": "F1",

        "count-as-correct": "CmdOrCtrl+C",
        "count-as-wrong": "CmdOrCtrl+X",
        "ignore-answer": "CmdOrCtrl+I",
        "add-solution": "CmdOrCtrl+Plus",
        "edit-test-item": "CmdOrCtrl+M",
        "wrap-up-test": "CmdOrCtrl+L",

        "save-input": "CmdOrCtrl+Enter"
    }
